class Solution {
    fun solution(n: Int): Int {
        var answer = n.toString()
        var res = 0
        for(i in 0 until answer.length){
            res += answer[i].toString().toInt()
                    
        
        }
        

        // [실행] 버튼을 누르면 출력 값을 볼 수 있습니다.
        

        return res
    }
}
