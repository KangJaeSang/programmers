class Solution {
    fun solution(n: Int): String {
        var answer = CharArray(n)
        
        for(i in 0 until n){
        if(i.rem(2) == 0){
            answer[i] = '수'
        }else {
            answer[i] = '박'
        }    
        }
        
        
        return answer.concatToString()
    }
}
